{
  "manifest_version": 2,
  "name": "ShowKey",
  "version": "0.0.1",
  "description": "Show key in browser",
  "author": "mgi166",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["scripts/contentScripts.bundle.js"]
    }
  ],
  "permissions": [
    "storage"
  ],
  "options_page": "views/options.html"
}
